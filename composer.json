{
    "name": "nexusphp/semi-finals",
    "type": "library",
    "description": "Removes T_FINAL token on-the-fly from final classes and methods in order to allow mocking in tests.",
    "license": "MIT",
    "authors": [
        {
            "name": "John Paul E. Balandan, CPA",
            "email": "paulbalandan@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-tokenizer": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.1",
        "nexusphp/cs-config": "^3.3",
        "nexusphp/tachycardia": "^1.3",
        "phpstan/phpstan": "^0.12.99",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Nexus\\StreamWrapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nexus\\StreamWrapper\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/NexusPHP/semi-finals/issues",
        "source": "https://github.com/NexusPHP/semi-finals.git",
        "slack": "https://nexusphp.slack.com"
    }
}
